          $(".hellouser").css({'display': 'none'});
            $(".hellob").css({'display': 'none'});
            $(".hellofade").css({'display': 'none'});
             $(".qdialogpage").css({'display': 'none'});
                   $(".pong").css({'display': 'none'});
                        $(".pongheader").css({'display': 'none'});
                            $(".toggle7").css({'display': 'block'});
                        $(".toggle6").css({'display': 'block'});
                    $(".toggle5").css({'display': 'block'});
                  $(".toggle4").css({'display': 'block'});
                         $(".helloc").css({'display': 'none'});  
                           $(".main").css({'backdrop-filter': 'blur(6px)',
               '-webkit-backdrop-filter': 'blur(6px)'});
        $(".searchcontainer2").css({'backdrop-filter': 'blur(6px)',
               '-webkit-backdrop-filter': 'blur(6px)',
               'background':'rgba(0,0,0,0.0)'});
        $(".sidebar").css({'backdrop-filter': 'blur(6px)',
               '-webkit-backdrop-filter': 'blur(6px)',
               'background':'rgba(0,0,0,0.0)'});
        $(".icon-bar").css({'backdrop-filter': 'blur(6px)',
               '-webkit-backdrop-filter': 'blur(6px)',
               'background':'rgba(0,0,0,0.0)'});
             
        $(".youtubeeffect").css({'display': 'block'}); 
        $(".orbstop").css({'display': 'block'}); 
        $(".youtubebackground").css({'display': 'block'});
        $(".helloc").css({'display': 'block','opacity':'0'});
    $(".capcut1").css({'display': 'none'});
                            $(".capcutcloud").css({'display': 'none'});
                               $(".cornerbutton3").css({'display': 'none'});
