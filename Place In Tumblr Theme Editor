<script>
    var myDate = new Date();
    var hrs = myDate.getHours(); 

    var greet;    
    if (hrs < 5) 
        greet = 'What are you doing up still, silly?';  
    else if (hrs >= 5 && hrs <= 12)
        greet = 'Good Morning {block:IfShowGreetingTitle}{Title}{/block:IfShowGreetingTitle}';
    else if (hrs >= 12 && hrs <= 17)
        greet = 'Good Afternoon {block:IfShowGreetingTitle}{Title}{/block:IfShowGreetingTitle}';
        
    else if (hrs >= 17 && hrs <= 24)
        greet = 'Good Evening {block:IfShowGreetingTitle}{Title}{/block:IfShowGreetingTitle}';
        
    else if (hrs >= 22 && hrs <= 24)
        greet = 'Goodnight';
        
 
    document.getElementById('lblGreetings').innerHTML =
        '<b>' + greet + '</b>';
</script> 

<script type="text/javascript">
    function updateClock(){
      var now = new Date();
      var dname = now.getDay(),
          mo = now.getMonth(),
          dnum = now.getDate(),
          yr = now.getFullYear(),
          hou = now.getHours(),
          min = now.getMinutes(),
          sec = now.getSeconds(),
          pe = "AM";
		  
          if(hou >= 12){
            pe = "PM";
          }
          if(hou == 0){
            hou = 12;
          }
          if(hou > 12){
            hou = hou - 12;
          }

          Number.prototype.pad = function(digits){
            for(var n = this.toString(); n.length < digits; n = 0 + n);
            return n;
          }

          var months = ["January", "February", "March", "April", "May", "June", "July", "Augest", "September", "October", "November", "December"];
          var week = ["Sunday", "Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday"];
          var ids = ["dayname", "month", "daynum", "year", "hour", "minutes", "seconds", "period"];
          var values = [week[dname], months[mo], dnum.pad(2), yr, hou.pad(2), min.pad(2), sec.pad(2), pe];
          for(var i = 0; i < ids.length; i++)
          document.getElementById(ids[i]).firstChild.nodeValue = values[i];
    }

    function initClock(){
      updateClock();
      window.setInterval("updateClock()", 1);
    }
    </script>

<script type="text/javascript">
<!--
var tickercontent=new Array();

tickercontent[0]='wave os 1.4';
